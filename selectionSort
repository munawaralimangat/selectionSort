function sel(arr){
    for(let i=0;i<arr.length-1;i++){

        for(let j=i+1;j<arr.length;j++){
            if (arr[i]>arr[j]) {
                let temp = arr[i]
                arr[i]=arr[j]
                arr[j]=temp
            }
        }
    }
    return arr
}

let arr = [100,44,77,10,1100,44,77,10,1]
let res = sel(arr)
console.log(arr)
